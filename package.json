{
  "name": "chessstate",
  "version": "1.0.2",
  "description": "A chess game state manager",
  "main": "ChessState.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --compilers ts:ts-node/register test test/**/*Test.ts",
    "build": "tsc",
    "bundle": "webpack --mode=production",
    "bundle debug": "webpack --mode=development",
    "help": "echo To bundle, `$ npm run bundle debug`",
    "start": "node ./bundles/ChessState.js",
    "super": "tsc; if [ 0 -eq $? ]; then webpack; npm start; fi;",
    "transpile": "babel src/Index.ts --out-file src/index-transpiled.js"
  },
  "files": [
    "bundles/ChessState.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NathanTan/ChessState.git"
  },
  "keywords": [
    "chess",
    "variants",
    "bughouse"
  ],
  "author": "Nathan Tan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NathanTan/ChessState/issues"
  },
  "homepage": "https://github.com/NathanTan/ChessState#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "ts-loader": "^5.3.1",
    "ts-node": "^8.0.2",
    "typescript": "^3.9.10",
    "mocha": "^5.2.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.1.2"
  }
}
